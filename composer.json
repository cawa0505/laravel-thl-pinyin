{
    "name": "cawa0505/laravel-thl-pinyin",
    "description": "Romanization tool for Traditional Chinese. A Laravel Wrapper for thl/pinyin",
    "keywords": [
        "laravel",
        "pinyin",
        "bopomofo",
        "chinese"
    ],
    "require": {
        "thl/pinyin": "~1.2",
        "php": "~7.0",
        "laravel/framework": "~5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "THL\\LaravelPinyin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "THL\\LaravelPinyin\\ServiceProvider"
            ],
            "aliases": {
                "Pinyin": "THL\\LaravelPinyin\\Facades\\Pinyin"
            }
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "jimmyyen",
            "email": "zeng.tw@gmail.com"
        }
    ]
}
